<template name="starsTemplate">
  <view class="stars-container">
    <view class="stars">
      <block wx:for="{{stars}}">
        <image wx:if="{{item == 1}}" class="star-img" src="/images/icon/star.png"></image>
        <image wx:else class="star-img" src="/images/icon/none-star.png"></image>
      </block>
    </view>
    <text class="star-score">{{average}}</text>
  </view>
</template>